                                triggerAlert('This order already accepted!', 'error');
                            }
                             //
                           // location.href = '{{  url(app()->getLocale()."/user/express-confirmed-order-seller-info") }}';
                            if(data.status.order_status == 0){
                                $.ajax({
                                    type: 'POST',
                                    url: '{{ url("/expressOrderConfirmation") }}',
                                    data: {
                                        '_token': "{{ csrf_token() }}",
                                        'orderId' : orderId
                                    },
                                    success: function (data) {

                                        //console.log(data);
                                         //triggerAlert('This order already accepted!', 'error');
                                       location.href = '{{  url(app()->getLocale()."/user/express-confirmed-order-seller-info") }}';

                                    },
                                    error: function(xhr, status, error) {
                                        var erroJson = JSON.parse(xhr.responseText);
                                    }
                                });
                            }

                        },
                        error: function(xhr, status, error) {
                            var erroJson = JSON.parse(xhr.responseText);
                        }
                    });

                }
            }else{
                 triggerAlert('Previous Order pending!', 'error');
            }
            }
            </script>
    </body>

